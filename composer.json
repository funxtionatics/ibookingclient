{
    "name": "funxtionatics/ibookingclient",
    "description": "A library that interfaces with the iBooking API",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "type": "library",
    "homepage": "http://swagger.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.18.0"
    },
    "autoload": {
        "psr-4": { "iBooking\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "iBooking\\Client\\" : "test/" }
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://bitbucket.org/funxtionatics/ibookingclient/"
        }
    ]
}
